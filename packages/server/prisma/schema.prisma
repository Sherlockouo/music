generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "sqlite"
  url          = "file:./musicInfo.db"
  relationMode = "prisma"
}

model Album {
  id                 Int                 @id
  json               String
  updatedAt          DateTime
  AlbumEditorialNote AlbumEditorialNote?
}

model AlbumEditorialNote {
  id    Int     @id @unique
  album Album   @relation(fields: [id], references: [id])
  en_US String? @map("en_US")
  zh_CN String? @map("zh_CN")
}

model Artist {
  id        Int        @id @unique
  json      String
  updatedAt DateTime
  ArtistBio ArtistBio?
}

